# App Configuration
VITE_APP_NAME=Mushroom Finder
VITE_APP_VERSION=1.0.0


# Firebase Configuration
VITE_FIREBASE_API_KEY=your-api-key-here
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=your-app-id

# Firebase Cloud Messaging
VITE_FIREBASE_VAPID_KEY=your-vapid-key-here

# Google Maps API Key
# Obté una API key des de: https://console.cloud.google.com/google/maps-apis/
# Habilita les APIs: Maps JavaScript API
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# OpenWeather API Key
# Obté una API key gratuïta (1000 crides/dia) des de: https://openweathermap.org/api
# 1. Crea un compte a https://home.openweathermap.org/users/sign_up
# 2. Verifica el teu email
# 3. Genera una API key a https://home.openweathermap.org/api_keys
VITE_OPENWEATHER_API_KEY=your_openweather_api_key_here

